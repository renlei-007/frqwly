page{background-color:#fff}.venues-detail{width:100%;overflow:auto;box-sizing:border-box}.venues-detail .venues_box{margin-top:20rpx;width:100%;box-sizing:border-box;padding:0 30rpx}.venues-detail .venues_box .venues_title{width:100%;font-size:36rpx;line-height:60rpx;font-weight:bolder;color:#1b1c1e;overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.venues-detail .venues_box .venues_info{font-size:32rpx;font-weight:400;line-height:64rpx;color:#1b1c1e;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-align:center;-webkit-align-items:center;align-items:center}.venues-detail .venues_box .venues_info_name{color:#666}.venues-detail .venues_box .venues_info .address{max-width:calc(100% - 218rpx);overflow:hidden;text-overflow:ellipsis;white-space:nowrap}.venues-detail .venues_box .venues_info .address_btn{width:108rpx;height:38rpx;margin-left:10rpx;border:2rpx solid #5d4ade;border-radius:8rpx;text-align:center;line-height:38rpx;color:#614ddf;font-size:22rpx}.venues-detail .venues_box .venues_infos{font-size:32rpx;font-weight:400;line-height:64rpx;color:#666}.venues-detail .venues_box .venues_infos text{color:#1b1c1e}.venues-detail .introduce{width:100%;box-sizing:border-box;padding:30rpx 0;line-height:40rpx;font-size:28rpx;color:#8b8b9c}.venues-detail .introduce .more{font-size:24rpx;line-height:40rpx;color:#7666e3;margin-left:6rpx}.dz_red{color:#df1414}.gray{background:#ccc!important}
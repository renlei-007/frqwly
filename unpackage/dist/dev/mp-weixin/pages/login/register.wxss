.register {
  width: 100%;
  height: 100%;
  background-color: #FFFFFF;
}
.register_box {
  width: 690rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding-top: 150rpx;
}
.register_box .log_int {
  width: 100%;
  height: 108rpx;
  background: #F2F5FA;
  border-radius: 16rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
}
.register_box .log_int input {
  width: 100%;
  height: 100%;
  line-height: 108rpx;
  color: #000;
  font-size: 34rpx;
}
.register_box .pass_int {
  margin-top: 30rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.register_box .pass_int input {
  width: 85%;
}
.register_box .pass_int .pass_see {
  width: 50rpx;
  height: 50rpx;
}
.register_box .log_btn {
  width: 690rpx;
  height: 90rpx;
  background: #FBA933;
  border-radius: 54rpx;
  text-align: center;
  line-height: 90rpx;
  margin-top: 64rpx;
  font-size: 34rpx;
  letter-spacing: 10rpx;
  color: #FFFFFF;
}
.register_box .code_box {
  margin-top: 30rpx;
  box-sizing: border-box;
  padding-right: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.register_box .code_box .code_inp {
  width: 50%;
}
.register_box .code_box .tips {
  font-size: 32rpx;
  color: #6446E1;
}


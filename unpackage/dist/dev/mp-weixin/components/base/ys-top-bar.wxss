.ys_top_bar {
  width: 100%;
  position: relative;
  height: 564rpx;
}
.ys_top_bar .wx_bar_box {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: 997;
  left: 0;
  box-sizing: border-box;
  color: #fff;
}
.ys_top_bar .wx_custom_box, .ys_top_bar .wx_search_box {
  width: 550rpx;
  height: 100%;
  position: relative;
}
.ys_top_bar .wx_logo_box, .ys_top_bar .wx_custom_gradient {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 0 0 30rpx;
  box-sizing: border-box;
}
